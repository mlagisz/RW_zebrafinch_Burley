@article{ ISI:000318291900001,
Author = {Featherston, R. and Jones, T. M. and Elgar, M. A.},
Title = {{Female resistance behaviour and progeny sex ratio in two Bradysia
   species (Diptera: Sciaridae) with paternal genome elimination}},
Journal = {{JOURNAL OF EVOLUTIONARY BIOLOGY}},
