@article{ ISI:000081289700008,
Author = {Schlupp, I and Waschulewski, M and Ryan, MJ},
Title = {{Female preferences for naturally-occurring novel male traits}},
Journal = {{BEHAVIOUR}},
Year = {{1999}},
Volume = {{136}},
Number = {{4}},
Pages = {{519-527}},
Month = {{MAY}},
Abstract = {{We report two independent cases of female preferences for novel male
   traits in two species of poeciliid fish, Poecilia latipinna and Poecilia
   mexicana. In both cases the preference predates the occurrence of the
   trait, lending strong support to the pre-existing bias hypotheses. This
   support is independent of the assumptions associated with phylogenetic
   inference. Unlike the two sexual species, the unisexual hybrid P.
   formosa had no detectable preference for the novel male traits.}},
