@article{ ISI:000430172900018,
Author = {Wang, Daiping and Forstmeier, Wolfgang and Ihle, Malika and Khadraoui,
   Mehdi and Jeronimo, Sofia and Martin, Katrin and Kempenaers, Bart},
Title = {{Irreproducible text-book ``knowledge{''}: The effects of color bands on
   zebra finch fitness}},
Journal = {{EVOLUTION}},
