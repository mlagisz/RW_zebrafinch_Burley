@article{ ISI:A1994PQ35100005,
Author = {BURLEY, NT and ENSTROM, DA and CHITWOOD, L},
Title = {{EXTRA-PAIR RELATIONS IN ZEBRA FINCHES - DIFFERENTIAL MALE SUCCESS
   RESULTS FROM FEMALE TACTICS}},
