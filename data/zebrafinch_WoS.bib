@article{ ISI:A1993MP63200007,
Author = {GUTZWILLER, KJ and MARCUM, HA},
Title = {{AVIAN RESPONSES TO OBSERVER CLOTHING COLOR - CAVEATS FROM WINTER POINT
   COUNTS}},
Journal = {{WILSON BULLETIN}},
Year = {{1993}},
Volume = {{105}},
Number = {{4}},
Pages = {{628-636}},
Month = {{DEC}},
Abstract = {{As part of a project to determine optimal methods for censusing winter
   birds with unlimited-distance point counts, we studied the influence of
   wearing a hunter-orange vest on richness estimates and species'
   detection probabilities. We did not find a significant difference in the
   number of species detected between apparel conditions (vest worn, vest
   not worn). However, separate detection probabilities for three species
   were associated with whether or not an orange vest was worn (Carolina
   Chickadee {[}Pants carolinensis, P = 0.075]; Tufted Titmouse {[}P.
   bicolor, P = 0.009]; American Goldfinch {[}Carduelis tristis, P =
   0.002]). For all three species, detection probabilities were lower when
   a vest was worn, suggesting that these species were repelled by the
   orange vest. Movement toward or away from a color, respectively, may
   generate inaccurate assessments of behavior, habitat use, and abundance.
   Knowledge about chromotropic responses to observer apparel will enable
   investigators to design more valid research. A clear benefit of improved
   designs will be more accurate inferences, which are crucial for
   conservation efforts and for advancing ornithological science.}},
