@inproceedings{ ISI:000403665100013,
Author = {Paci, Patrizia and Mancini, Clara and Price, Blaine A.},
Book-Group-Author = {{ACM}},
Title = {{Designing for Wearability in Animal Biotelemetry}},
Booktitle = {{PROCEEDINGS OF THE THIRD INTERNATIONAL CONFERENCE ON ANIMAL-COMPUTER
   INTERACTION, ACI 2016}},
Year = {{2016}},
Note = {{3rd International Conference on Animal-Computer Interaction (ACI),
   Milton Keynes, ENGLAND, NOV 15-17, 2016}},
Abstract = {{This research presents a preliminary study conducted on a cat fitted
   with biotelemetry devices. The aim was to explore the feline's
   wearability experience of bearing off-the-shelf products. The cat's
   reactions to the device presence were recorded and findings suggest the
   need for a design approach centred on the wearer. A wearer-centred
   framework to inform the design of biotelemetry interventions for animals
   is then proposed.}},
