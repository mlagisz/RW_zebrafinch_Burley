Address = {{JOURNALS DEPT, 2001 EVANS RD, CARY, NC 27513 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Wolfenbarger, LL (Reprint Author), Univ Maryland, Dept Zool, College Pk, MD 20742 USA.
   Cornell Univ, Neurobiol \& Behav Sect, Ithaca, NY 14853 USA.}},
DOI = {{10.1093/beheco/10.1.80}},
ISSN = {{1045-2249}},
EISSN = {{1465-7279}},
Keywords = {{Cardinalis cardinalis; plumage coloration; laying date; mate quality;
