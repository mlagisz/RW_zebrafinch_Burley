@article{ ISI:A1992HW60700010,
Author = {ROST, R and LANG, E and LEY, HW},
Title = {{IS THERE A SURPLUS OF MALES IN BLACK WOODPECKER NESTLINGS
   (DRYOCOPUS-MARTIUS)}},
Journal = {{JOURNAL FUR ORNITHOLOGIE}},
Year = {{1992}},
Volume = {{133}},
Number = {{2}},
Pages = {{203-208}},
Month = {{APR}},
Abstract = {{A population of Black Woodpeckers was studied in an area of about 200
   km2 in southern Germany from 1978 up to 1991. Once a year the nesting
   trees were visited to record the age and number of young and to ring the
   nestlings. In 1979 the sex of seven birds (two broods) was determined
   additionally (crown feathers showing red from tenth day in male,
   bluish-black in female - only nape is red). From 1980 onwards the young
   of all broods censused were sexed (445 birds; 140 broods; age of the
   birds: 14 to 20 days). In contrast to several studies which had pointed
   to a male-biased secondary sex ratio in the Black Woodpecker, but
   corresponding to findings in various bird species sexually monomorphic
   in size, neither the overall sex ratio (0,92 male:1 female; pooled data
   from 1979 to 1991) nor the ratios of single years showed any significant
   deviations from 1:1. Likewise, we did not find any differences between
   the sex ratios of Black Woodpecker nestlings reported in the literature
   and a balanced ratio. For these reasons it is not tenable any longer to
   speak about a surplus of male in Black Woodpecker nestlings.}},
